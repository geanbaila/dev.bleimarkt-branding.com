@font-face {
  font-family: raleway-bold;
  src: url('../font/Raleway/static/Raleway-Bold.ttf');
}
@font-face {
  font-family: raleway-medium; 
  src: url('../font/Raleway/static/Raleway-Medium.ttf');
}
@font-face {
  font-family: raleway-regular; 
  src: url('../font/Raleway/static/Raleway-Regular.ttf');
}
@font-face {
  font-family: poppins-bold; 
  src: url('../font/Poppins/Poppins-Bold.ttf');
}

body{
    font-family: "raleway-regular";
    font-size: 14px;
    color: #8b8171 !important;
}

.header{
  height: 74px;
  background-color: #A9CEA2;
  margin-bottom: 38px !important;
}

#headerMenu{
    width: 100%;
    height: 74px;/*58px; */
    font-size: 16px !important;
    box-shadow: rgba(0, 0, 0, 0.2) 0px 4px 20px 0px;
}

#headerMenu ul{
  margin-top: 16px;
  border-top: 2px solid #A9CEA2;
  float: right;
}

#headerMenu ul li{
  margin-top:0px !important; 
  height: 100%;
}

#headerMenu ul li a.nav-link{
  margin-top: 0px !important;
  padding-top: 0px !important;
  margin-bottom: 0px !important;
  padding-bottom: 0px !important;
}
#headerMenu ul li:not(:last-child):hover, 
#headerMenu ul li.active{
    border-top: 11px solid #A9CEA2;
}
.row >* {
  padding-right:0px !important;
  padding-left:0px !important;
}
.container{
  padding: 0px 27px!important;
}

