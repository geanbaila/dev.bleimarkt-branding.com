.header-menu{
  display: none;
}
@media (max-width: 700px) {
  #headerMenu{
    display: none;
  }
  .header-menu{
    display: block;
  }
}

#sidebarMenu{
  width: 216px;
  padding: 0px 27px;
  /*border: 3px solid#A9CEA2 ;*/
  overflow-y: auto;
  font-family: "raleway-bold";
}

#sidebarMenu ul li a{
  padding: 0px 39px;
}

.feather {
  width: 16px;
  height: 16px;
  vertical-align: text-bottom;
}


.sidebar {
  position: fixed;
  padding: 48px 0 0; 
  z-index: 9999; 
  top: 0px;
  bottom: 0;
  left: 0;
  box-shadow: inset -1px 0 0 rgba(0, 0, 0, .1);
}

@media (max-width: 767.98px) {
  .sidebar {
    top: 3rem;
  }
  .navbar-brand{
    padding-top: 20px;
    background-color: transparent !important;
    border: 0px solid transparent !important;
  }
}


.sidebar-sticky {
  position: relative;
  top: 0;
  height: calc(100vh - 48px);
  padding-top: .5rem;
  overflow-x: hidden;
  overflow-y: auto; 
}

.sidebar .nav-link.active {
  color: #FFFFFF;
}

.sidebar .nav-link:hover .feather,
.sidebar .nav-link.active .feather {
  color: inherit;
}

.sidebar-heading {
  font-size: .75rem;
  text-transform: uppercase;
}


#navbar-brand{
  border: 15px solid red;
}

.navbar-brand {
  width: 216px;
  padding-top: .75rem;
  padding-bottom: .75rem;
  font-size: 1rem;
  background-color: rgba(0, 0, 0, .25);
  box-shadow: inset -1px 0 0 rgba(0, 0, 0, .25);
}


.navbar .navbar-toggler {
  /*top: .25rem;
  right: 1rem;*/
  border: 0px solid transparent;
}





#card-uberuns{
  border: 0px;
  line-height: 22px;
}

#card-uberuns .card-body{
  padding-left: 47px;
  padding-right: 45px;
  
  /*
  padding-bottom: 36px;
  border: 1px dotted green;*/
}
#card-uberuns .card-body .card-title{
  padding-top: 12px;
  font-size: 31px;
  font-family: "poppins-bold";
  font-stretch: ultra-condensed !important;
  transform: scaleY(1.09);
}

#card-uberuns .card-body .card-text{
  margin-top:20px;
  font-size: 13.3px;
  font-family: "raleway-medium";
  font-stretch: ultra-condensed;  
  transform: scaleY(1.09);
}
#card-uberuns .card-body .card-text p:last-child{
  margin-top: 22px;
}

@media (min-width: 1020px) {
    #card-uberuns{
        width: 400px;
        height: 414px;
        position: absolute !important;
        margin: -541px 40px 0px 582px !important;
    }
    .downCard{
        /*background-color: gray !important;*/
        margin: 100px 0px !important;
    }
}
@media (max-width: 992px) {
  #card-uberuns{
    position: relative;
    margin-top: -40px;
    margin-bottom: 40px;
  }
}

.ean-rounded{
  border-radius: 0px 20px 20px 20px;
}

